        INPUT :- [2]

#include <stdio.h>
int main() {
    int i,j;
    for(i=0; i<=5; i++)
    {
        for(j=5; j>=i; j--)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
} 

      OUTPUT :- [2]

 
